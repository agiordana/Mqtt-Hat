<?xml version="1.0"?>
<set type="in5">
<attribute name="type" value="booleanin"/>
<attribute name="capename" value="IN5"/>
<attribute name="gpioname" value="gpio27"/>
<attribute name="gpiopath" value="/sys/class/gpio/gpio27"/>
<attribute name="waitfor" value="0"/>
<attribute name="event" value="from/$MQTT_DOMAIN/$MQTT_SUBDOMAIN/gpio/IN5"/>
<attribute category="script" name="cmd2" value="echo in > /sys/class/gpio/gpio27/direction"/>
<attribute category="script" name="cmd3" value="echo both > /sys/class/gpio/gpio27/edge"/>
</set>
